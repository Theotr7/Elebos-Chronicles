{% extends 'base.html.twig' %}

{% block title %}Ajouter un booster
{% endblock %}

{% block body %}
	<h1>Nouveau booster</h1>

	<div class="scroll-wrapper">
		<div class="scroll-background" style="background-image: url('{{ asset('images/ui/parchemin_test.png') }}');">
			<form method="post" class="scroll-form">
				{{ form_start(form) }}
				{{ form_widget(form) }}
				<button type="submit" class="btn-primary">Enregistrer</button>
				{{ form_end(form) }}
			</form>
		</div>
	</div>
{% endblock %}
